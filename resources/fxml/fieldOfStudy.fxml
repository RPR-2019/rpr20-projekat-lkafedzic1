<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" prefHeight="132.0" prefWidth="252.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.project.FieldOfStudyController">
   <columnConstraints>
      <ColumnConstraints maxWidth="117.0" minWidth="29.0" prefWidth="57.0" />
      <ColumnConstraints maxWidth="185.0" minWidth="97.0" prefWidth="157.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Title:" />
      <TextField fx:id="fldTitle" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="26.0" prefWidth="173.0" GridPane.columnIndex="1" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
        <buttons>
          <Button fx:id="btnAdd" defaultButton="true" mnemonicParsing="false" onAction="#actionAddNewField" text="Add" />
            <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" onAction="#actionCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
